28430 silly install resolved [ { name: 'async',
28430 silly install resolved     description: 'Higher-order functions and common patterns for asynchronous code',
28430 silly install resolved     main: './lib/async',
28430 silly install resolved     author: { name: 'Caolan McMahon' },
28430 silly install resolved     version: '0.9.0',
28430 silly install resolved     repository: { type: 'git', url: 'https://github.com/caolan/async.git' },
28430 silly install resolved     bugs: { url: 'https://github.com/caolan/async/issues' },
28430 silly install resolved     licenses: [ [Object] ],
28430 silly install resolved     devDependencies: { nodeunit: '>0.0.0', 'uglify-js': '1.2.x', nodelint: '>0.0.0' },
28430 silly install resolved     jam: { main: 'lib/async.js', include: [Object] },
